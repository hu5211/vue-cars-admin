AAA,QAAA,EAAA,IADA;AAEA,UAAA,OAAA,EAAA,SAFA;AAGA,UAAA,OAAA,EAAA;AAHA,SADA;AAPA;AAlBA,KAAA;AAkCA,GAhDA;AAiDA,EAAA,OAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,WAAA,YAAA;;AACA,UAAA,KAAA,EAAA,EAAA;AACA,aAAA,WAAA;AACA;AACA,KANA;AAOA;AACA,IAAA,YARA,0BAQA;AAAA;;AACA;AACA,UAAA,KAAA,IAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,eAAA,KAAA;AACA;;AACA,MAAA,SAAA,GAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AACA,YAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,IAAA,GAAA,IAAA;AACA;AACA,OALA;AAMA,KAnBA;AAoBA;AACA,IAAA,WArBA,yBAqBA;AAAA;;AACA,MAAA,aAAA,CAAA;AAAA,QAAA,EAAA,EAAA,KAAA;AAAA,OAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,YAAA,IAAA,GAAA,QAAA,CAAA,IAAA;;AACA,YAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,GAAA,IAAA;AACA,UAAA,MAAA,CAAA,YAAA,GAAA,IAAA,CAAA,MAAA;AACA;AACA,OANA;AAOA,KA7BA;AA8BA;AACA,IAAA,MA/BA,kBA+BA,QA/BA,EA+BA;AAAA;;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,EAAA,GAAA,MAAA,CAAA,IAAA,EAAA,GAAA,MAAA,CAAA,GAAA,EAAA;AACA,SAFA,MAEA;AACA,iBAAA,KAAA;AACA;AACA,OANA;AAOA,KAvCA;AAwCA;AACA,IAAA,GAzCA,iBAyCA;AAAA;;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,KAAA,YAAA;AACA,MAAA,QAAA,CAAA,KAAA,IAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA,QAAA,CAAA;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,KAAA;AACA,OANA;AAOA,KAlDA;AAmDA;AACA,IAAA,IApDA,kBAoDA;AAAA;;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,KAAA,YAAA;AACA,UAAA,WAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA,CAAA,CAAA;AACA,MAAA,WAAA,CAAA,EAAA,GAAA,KAAA,EAAA;AACA,MAAA,SAAA,CAAA,WAAA,CAAA,CAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,OAAA,EAAA,QAAA,CAAA;AAFA,SAAA;;AAIA,QAAA,MAAA,CAAA,KAAA;AACA,OANA;AAOA,KA/DA;AAgEA;AACA,IAAA,KAjEA,iBAiEA,QAjEA,EAiEA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,WAAA;AACA,WAAA,YAAA,GAAA,EAAA;AACA,KApEA;AAqEA,IAAA,KArEA,mBAqEA;AACA;AACA,WAAA,KAAA,CAAA,oBAAA,EAAA,KAAA;AACA;AAxEA,GAjDA;AA2HA,EAAA,KAAA,EAAA;AACA,IAAA,WAAA,EAAA;AACA,MAAA,OADA,mBACA,QADA,EACA,QADA,EACA;AACA,aAAA,aAAA,GAAA,QAAA;AACA;AAHA;AADA;AA3HA,CAAA","sourcesContent":["<template>\r\n  <div class=\"cars-dialog-center\">\r\n    <!-- 弹窗 -->\r\n    <el-dialog\r\n      title=\"新增车辆品牌\"\r\n      :visible.sync=\"dialogVisible\"\r\n      :close-on-click-modal=\"false\"\r\n      @close=\"close\"\r\n      @opened=\"opened\"\r\n    >\r\n      <!-- 内容区 -->\r\n      <el-form\r\n        ref=\"form\"\r\n        :rules=\"rules\"\r\n        :model=\"form\"\r\n        label-width=\"110px\"\r\n        size=\"small\"\r\n        :inline-message=\"true\"\r\n      >\r\n        <el-form-item label=\"品牌中文\" prop=\"nameCh\">\r\n          <el-input v-model=\"form.nameCh\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"车辆英文\" prop=\"nameEn\">\r\n          <el-input v-model=\"form.nameEn\"></el-input>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"LOGO\" prop=\"imgUrl\">\r\n          <div class=\"upload-img-wrap\">\r\n            <div class=\"upload-img\">\r\n              <img v-show=\"logo_current\" :src=\"logo_current\" />\r\n            </div>\r\n            <div class=\"img-list\" prop=\"logo\">\r\n              <li v-for=\"item in logo\" :key=\"item.id\" @click=\"logo_current=item.img\">\r\n                <img :src=\"item.img\" alt=\"item.name\" />\r\n              </li>\r\n            </div>\r\n          </div>\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"禁启用\" prop=\"status\">\r\n          <el-radio-group v-model=\"form.status\">\r\n            <el-radio\r\n              v-for=\"item in radio_status\"\r\n              :label=\"item.value\"\r\n              :key=\"item.id\"\r\n            >{{ item.label}}</el-radio>\r\n          </el-radio-group>\r\n        </el-form-item>\r\n\r\n        <el-form-item>\r\n          <el-button type=\"danger\" @click=\"submit('form')\">确定</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { BrandLogo, BrandAdd, BrandDetailed, BrandEdit } from \"@/api/brand\";\r\nexport default {\r\n  name: \"\",\r\n  components: {},\r\n  props: {\r\n    flagVisible: {\r\n      type: Boolean,\r\n      default: false\r\n    },\r\n    id: {\r\n      type: String,\r\n      default: \"\"\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      // 弹窗显示关闭标记\r\n      dialogVisible: false,\r\n      // 表单\r\n      form: {\r\n        nameCh: \"\",\r\n        nameEn: \"\",\r\n        imgUrl: \"\",\r\n        status: \"\",\r\n        content: \"\"\r\n      },\r\n      //logo\r\n      logo: [],\r\n      //选中的logo\r\n      logo_current: \"\",\r\n      //状态\r\n      radio_status: this.$store.state.config.radio_status,\r\n      //验证表单规则\r\n      rules: {\r\n        nameCh: [\r\n          { required: true, message: \"请输入中文品牌\", trigger: \"blur\" }\r\n        ],\r\n        nameEn: [\r\n          { required: true, message: \"请输入英文品牌\", trigger: \"blur\" }\r\n        ],\r\n        status: [\r\n          {\r\n            required: true,\r\n            message: \"请至少选择一个\",\r\n            trigger: \"change\"\r\n          }\r\n        ]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    opened() {\r\n      this.getBrandLogo();\r\n      if (this.id) {\r\n        this.getDetailed();\r\n      }\r\n    },\r\n    //获取logo\r\n    getBrandLogo() {\r\n      //存在数据时不再请求接口\r\n      if (this.logo.length !== 0) {\r\n        return false;\r\n      }\r\n      BrandLogo().then(response => {\r\n        const data = response.data;\r\n        if (data) {\r\n          this.logo = data;\r\n        }\r\n      });\r\n    },\r\n    //获取详情\r\n    getDetailed() {\r\n      BrandDetailed({ id: this.id }).then(respones => {\r\n        const data = respones.data;\r\n        if (data) {\r\n          this.form = data;\r\n          this.logo_current = data.imgUrl;\r\n        }\r\n      });\r\n    },\r\n    //提交\r\n    submit(formName) {\r\n      this.$refs[formName].validate(valid => {\r\n        if (valid) {\r\n          this.id ? this.edit() : this.add();\r\n        } else {\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    //添加\r\n    add() {\r\n      this.form.imgUrl = this.logo_current;\r\n      BrandAdd(this.form).then(response => {\r\n        this.$message({\r\n          type: \"success\",\r\n          message: response.message\r\n        });\r\n        this.close();\r\n      });\r\n    },\r\n    //编辑\r\n    edit() {\r\n      this.form.imgUrl = this.logo_current;\r\n      const requestData = JSON.parse(JSON.stringify(this.form));\r\n      requestData.id = this.id;\r\n      BrandEdit(requestData).then(response => {\r\n        this.$message({\r\n          type: \"success\",\r\n          message: response.message\r\n        });\r\n        this.close();\r\n      });\r\n    },\r\n    //重置表单\r\n    reset(formName) {\r\n      this.$refs[formName].resetFields();\r\n      this.logo_current = \"\";\r\n    },\r\n    close() {\r\n      // 反向修改\r\n      this.$emit(\"update:flagVisible\", false);\r\n    }\r\n  },\r\n  watch: {\r\n    flagVisible: {\r\n      handler(newValue, oldValue) {\r\n        this.dialogVisible = newValue;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.el-input {\r\n  width: 40%;\r\n}\r\n</style>"],"sourceRoot":"src/components/dialog"}]}